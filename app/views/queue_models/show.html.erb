<%- model_class = QueueModel -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @queue_model.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:gps_lon) %>:</strong></dt>
  <dd><%= @queue_model.gps_lon %></dd>
  <dt><strong><%= model_class.human_attribute_name(:gps_lat) %>:</strong></dt>
  <dd><%= @queue_model.gps_lat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @queue_model.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image) %>:</strong></dt>
  <dd><%= @queue_model.image %></dd>
  <dt><strong><%= model_class.human_attribute_name(:num_customer) %>:</strong></dt>
  <dd><%= @queue_model.num_customer %></dd>
  <dt><strong><%= model_class.human_attribute_name(:last_customer) %>:</strong></dt>
  <dd><%= @queue_model.last_customer %></dd>
  <dt><strong><%= model_class.human_attribute_name(:average_time) %>:</strong></dt>
  <dd><%= @queue_model.average_time %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              queue_models_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_queue_model_path(@queue_model), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              queue_model_path(@queue_model),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
